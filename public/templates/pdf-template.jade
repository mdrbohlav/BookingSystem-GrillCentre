doctype html
html
    head
        title= title
        //link(rel='stylesheet', href= style)
        style.
            html {
                font-family: sans-serif;
                -ms-text-size-adjust: 100%;
                -webkit-text-size-adjust: 100%;
            }

            body {
                margin: 0
            }

            a {
                background: transparent
            }

            a:active,
            a:hover {
                outline: 0
            }

            b,
            strong {
                font-weight: bold
            }

            small {
                font-size: 80%
            }

            sub,
            sup {
                font-size: 75%;
                line-height: 0;
                position: relative;
                vertical-align: baseline
            }

            sup {
                top: -.5em
            }

            sub {
                bottom: -.25em
            }

            img {
                border: 0
            }

            svg:not(:root) {
                overflow: hidden
            }

            hr {
                -moz-box-sizing: content-box;
                -webkit-box-sizing: content-box;
                -moz-box-sizing: content-box;
                box-sizing: content-box;
                height: 0
            }

            pre {
                overflow: auto
            }

            code,
            kbd,
            pre,
            samp {
                font-family: monospace, monospace;
                font-size: 1em
            }

            table {
                border-collapse: collapse;
                border-spacing: 0
            }

            td,
            th {
                padding: 0
            }

            a {
                display: inline-block;
                position: relative;
                text-decoration: none
            }

            html {
                -webkit-box-sizing: border-box;
                -moz-box-sizing: border-box;
                box-sizing: border-box
            }

            html {
                font-size: 100%;
                -webkit-text-size-adjust: 100%;
                -ms-text-size-adjust: 100%;
                height: 100%;
                width: 100%
            }

            body {
                background: #fff;
                overflow-x: hidden;
                color: #000;
                font-family: "Times New Roman", Times, serif;
                width: 100%;
                height: 100%;
                position: relative;
                font-size: 12px;
            }

            table {
                margin: 0;
                width: 100%;
            }

            table a {
                color: #000;
                font-style: italic;
            }

            thead {
                display: none;
            }

            h1,
            h2,
            ul,
            ol {
                margin: 0;
                padding-top: 0;
            }

            h1 {
                font-size: 15px;
                margin-bottom: 5px;
                text-align: center;
            }

            h2 {
                font-size: 12px;
                margin-top: 12px;
                text-align: center;
            }

            p {
                margin: 0;
            }

            table + p,
            ul + p,
            ol + p,
            p + table,
            p + p {
                margin-top: 12px;
            }


    body!= text
